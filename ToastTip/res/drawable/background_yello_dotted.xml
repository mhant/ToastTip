<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item >
        <shape>
            <solid android:color="@color/yellow"/>

            <stroke
                android:dashGap="6dp"
                android:dashWidth="4dp"
                android:width="2dip"
                android:color="@color/grey_border" />

            <padding
                android:bottom="2dp"
                android:left="2dp"
                android:right="2dp"
                android:top="2dp" />

        </shape>
    </item>
</layer-list>
